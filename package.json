{
  "name": "orders",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:tailwind": "tailwindcss --input ./src/css/tailwind-input.css --output ./src/css/tailwind-output.css --no-autoprefixer && tailwind-rn -i ./src/css/tailwind-output.css -o ./src/css/tailwind.json",
    "dev:tailwind": "concurrently \"tailwindcss --input ./src/css/tailwind-input.css --output ./src/css/tailwind-output.css --no-autoprefixer --watch\" \"tailwind-rn -i ./src/css/tailwind-output.css -o ./src/css/tailwind.json -w\""
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/material-top-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "babel-plugin-inline-import": "^3.0.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.67.3",
    "react-native-pager-view": "^5.4.11",
    "react-native-safe-area-context": "^4.1.2",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^3.1.1",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.0.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "postcss": "^8.4.8",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.23"
  },
  "jest": {
    "preset": "react-native"
  }
}
